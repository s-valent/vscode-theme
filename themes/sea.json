	"tokenColors": [
		{
			"scope": "comment, punctuation.definition.comment",
			"settings": {
				"foreground": "#768390"
			}
		},
		{
			"scope": "variable, entity.other.inherited-class",
			"settings": {
				"foreground": "#D4D4D4"
			}
		},
		{
			"scope": "keyword, storage.type, storage.modifier",
			"settings": {
				"foreground": "#C594C5"
			}
		},
		{
			"scope": "keyword.operator, constant.other.color, punctuation.definition.tag, punctuation.separator.inheritance.php, punctuation.definition.tag.html, punctuation.definition.tag.begin.html, punctuation.definition.tag.end.html, punctuation.section.embedded, keyword.other.template, keyword.other.substitution, entity.name.function.operator.member",
			"settings": {
				"foreground": "#a0a0a0"
			}
		},
		{
			"scope": "entity.name.tag, meta.tag.sgml, markup.deleted.git_gutter, support.type.exception",
			"settings": {
				"foreground": "#EB606B"
			}
		},
		{
			"scope": "entity.name.function, meta.function-call.python, variable.function, support.function, keyword.other.special-method, meta.block-level, meta.function.decorator, support.type",
			"settings": {
				"foreground": "#6699CC"
			}
		},
		{
			"scope": "meta.function.definition entity.name.function, meta.function entity.name.function.lua, meta.function support.function.magic.python, entity.name.function.python, entity.name.function.julia, entity.name.function.definition, meta.definition.function, meta.definition.method, entity.name.type.struct, entity.name.type.class, entity.name.type.enum, entity.name.type.alias",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "entity.name.function.c, entity.name.function.call, meta.definition.function.body, meta.parameter-clause",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"scope": "meta.function-call.arguments, punctuation.definition.arguments",
			"settings": {
				"foreground": "#D4D4D4"
			}
		},
		{
			"scope": "support.other.variable, string.other.link",
			"settings": {
				// "foreground": "#F2777A"
				"foreground": "#cd5e32"
			}
		},
		{
			"scope": "constant.numeric, constant.language, support.constant, constant.other.caps, constant.other.ellipsis, variable.other.constant, storage.type.imaginary",
			"settings": {
				"foreground": "#FAC863"
			}
		},
		{
			"scope": "variable.parameter, keyword.other.unit, constant.character, constant.character.escape, punctuation.definition.interpolation, constant.other.placeholder",
			"settings": {
				"foreground": "#F99157"
			}
		},
		{
			"scope": "support.type.property-name, punctuation.support.type.property-name",
			"settings": {
				"foreground": "#5FB3B3"
			}
		},
		{
			"scope": "string, punctuation.definition.string, constant.other.symbol, constant.other.key, markup.heading, markup.inserted.git_gutter, meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js",
			"settings": {
				"foreground": "#99C794"
			}
		},
		{
			"scope": "entity.name.class, entity.name.type.class, support.class, support.orther.namespace.use.php, meta.use.php, support.other.namespace.php, markup.changed.git_gutter",
			"settings": {
				"foreground": "#5FB3B3"
			}
		},
		{
			"scope": "entity.name.module.js, variable.import.parameter.js, variable.other.class.js",
			"settings": {
				"foreground": "#E15A60"
			}
		},
		{
			"scope": "variable.language, support.variable.magic",
			"settings": {
				"foreground": "#E15A60",
			}
		},
		{
			"scope": "entity.name.method.js",
			"settings": {
				"foreground": "#D8DEE9"
			}
		},
		{
			"scope": "meta.class-method.js entity.name.function.js, variable.function.constructor",
			"settings": {
				"foreground": "#D8DEE9"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#BB80B3"
			}
		},
		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#99C794"
			}
		},
		{
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#E15A60"
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#BB80B3"
			}
		},
		{
			"scope": "entity.name.type",
			"settings": {
				"foreground": "#5FB3B3"
			}
		},
		{
			"scope": "*url*, *link*, *uri*",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "constant.numeric.line-number.find-in-files - match",
			"settings": {
				"foreground": "#AB7967"
			}
		},
		{
			"scope": "entity.name.filename.find-in-files",
			"settings": {
				"foreground": "#99C794"
			}
		},
		{
			"scope": "tag.decorator.js entity.name.tag.js, tag.decorator.js punctuation.definition.tag.js",
			"settings": {
				"foreground": "#6699CC",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "source.js constant.other.object.key.js string.unquoted.label.js",
			"settings": {
				"foreground": "#E15A60",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#6796E6"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#CD9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#F44747"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#BB80B3"
			}
		},
		{
			"scope": "source.js entity.other.attribute-name.js",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "text.html.basic entity.other.attribute-name.html",
			"settings": {
				"fontStyle": "italic"
			}
		}
	]
}
